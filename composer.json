    "keywords": ["symfony", "feed", "xml", "atom"],
    "homepage": "https://github.com/Nek-/FeedBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxime Veber",
            "email": "nek.dev+github@gmail.com",
            "homepage": "http://www.nekland.fr"
        },
        {
            "name": "Nekland Community",
            "homepage": "https://github.com/Nek-/FeedBundle/contributors"
        }
